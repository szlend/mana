<h1 class="text-center">Game name: <%= @name %></h1>
<canvas id="game" width="303" height="303" data-name="<%= @name %>"></canvas>

<div id="game-x"></div>
<div id="game-y"></div>

<div>
  Users:
  <ul>
    <%= for user <- @users do %>
    <li>
      <span><%= user.username %></span>
    </li>
    <% end %>
  </ul>
</div>
